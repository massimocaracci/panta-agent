# DeepSeek API Configuration
# Replace this with your actual API key or set the DEEPSEEK_API_KEY environment variable
deepseek.api.key=your-api-key-here
deepseek.api.url=https://api.deepseek.com/v1/chat/completions

# Server Configuration
server.port=8080
spring.application.name=panta-agent-ai
logging.level.com.pantasoft.simpleagent=DEBUG

# OpenAPI Configuration
springdoc.api-docs.path=/api-docs
springdoc.swagger-ui.path=/swagger-ui.html
springdoc.swagger-ui.operationsSorter=method
springdoc.swagger-ui.tagsSorter=alpha 